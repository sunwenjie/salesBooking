body {
  /*position: relative;*/
  background-color: #fff;
  /*background-image: url(../img/grid-18px-masked.png);*/
  background-repeat: repeat-x;
  background-position: 0 40px;
}
a:focus, input[type=button] ,input[type=submit] {
         outline:none;
}
.load-item{padding-top: 60px; border-bottom: 1px #ccc dashed; padding-bottom: 40px;}
#overview{padding-top: 90px;}
#header{border: none;}
.jumbotron .inner{text-align: center;}
.jumbotron h1 {
  font-size: 81px;
  font-weight: bold;
  letter-spacing: -1px;
  line-height: 1;
  margin-bottom: 9px;
  color: #000;
}
.masthead p {
  font-size: 30px;
  line-height: 36px;
  margin-left: 5%;
  margin-right: 5%;
}
/*.index-pane{min-height: 480px;}*/

.subhead h1 {
  font-size: 40px;
}
.subhead p{font-size: 16px;}
.subnav-fixed {
  border-color: #D5D5D5;
  border-radius: 0 0 0 0;
  border-width: 0 0 1px;
  box-shadow: 0 1px 0 #FFFFFF inset, 0 1px 5px rgba(0, 0, 0, 0.1);
  left: 0;
  position: fixed;
  right: 0;
  top: 40px;
  z-index: 1020;
}
.subnav {
  background-color: #EEEEEE;
  border-color: #D5D5D5;
  border-radius: 0 0 0 0;
  border-width: 1px;
  /*box-shadow: 0 1px 0 #FFFFFF inset, 0 1px 5px rgba(0, 0, 0, 0.1);*/
  width: 100%;
}
.subnav-fixed .nav {
  margin: 0 auto;
  width: 960px;
}

footer{ 
  position: fixed;
  bottom: 20px;}
section {
  padding-top: 60px;
}


.com { color: #93a1a1; }
.lit { color: #195f91; }
.pun, .opn, .clo { color: #93a1a1; }
.fun { color: #dc322f; }
.str, .atv { color: #D14; }
.kwd, .linenums .tag { color: #1e347b; }
.typ, .atn, .dec, .var { color: teal; }
.pln { color: #48484c; }

.prettyprint {
  padding: 8px;
  background-color: #f7f7f9;
  border: 1px solid #e1e1e8;
}
.prettyprint.linenums {
  -webkit-box-shadow: inset 40px 0 0 #fbfbfc, inset 41px 0 0 #ececf0;
     -moz-box-shadow: inset 40px 0 0 #fbfbfc, inset 41px 0 0 #ececf0;
          box-shadow: inset 40px 0 0 #fbfbfc, inset 41px 0 0 #ececf0;
}

/* Specify class=linenums on a pre to get line numbering */
ol.linenums {
  margin: 0 0 0 33px; /* IE indents via margin-left */
} 
ol.linenums li {
  padding-left: 12px;
  color: #bebec5;
  line-height: 18px;
  text-shadow: 0 1px 0 #fff;
}

.navbar-fixed-top{margin: 0;}
.dl-horizontal dt {
  clear: left;
  float: left;
  text-align: right;
  width: 120px;
  font-size: 14px;
}

.dl-horizontal dd {
  margin-left: 130px;
}

.tab-content{padding-top: 20px;}
.h200{height: 200px;}
.nav-pills > li > a{border-radius:5px; text-decoration: none;}

.nav-pills > li{float: left;}
.doc-item-title{color: #f00;padding-left: 20px;}
/*.prettyprint{min-height: 80px;}*/
.cle{clear: both;}

.scrollspy-example {
  height: 200px;
  overflow: auto;
  position: relative;
}
#cont{position: relative;}
.datil-intro ol{padding-left: 30px; margin-bottom: 10px;}
.big_section{width: 100%;}

