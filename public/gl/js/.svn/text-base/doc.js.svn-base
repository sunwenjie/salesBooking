   //for nav
$(function() {
 $('.load-item').each(function(index,ele){
 	var src = $(ele).attr('data-src');
 	//$(ele).load(src,DOC.loadCallback);
 	$.ajax({
 		url : src,
 		success : function(data){
 			$(ele).html(data);
 			DOC.loadCallback();
 		}
 	})
 });
 DOC.loadCallback.itemLength = $('.load-item').length;

 // window.prettyPrint && prettyPrint();
 // $(".chzn-select").chosen();

})

DOC = {}
DOC.init = function(){
 	// make code pretty
	DOC.pretty();
	//console.info($('.nav-scrollspy'))
	//$('#header').scrollspy();

}
DOC.header = function(){

}
DOC.pretty = function(){
 $('pre').each(function(index,element){
    var html = $(element).html();
    $(element).html(html.replace(/</g,'&lt;').replace(/>/g,'&gt;'));
 });
 window.prettyPrint && prettyPrint();

}
DOC.loadCallback = function(){
	DOC.loadCallback.count++;
	if(DOC.loadCallback.count == DOC.loadCallback.itemLength){
		DOC.init();
	}
}
DOC.loadCallback.count = 0;