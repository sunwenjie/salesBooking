<div class="table_white">
      <table id="client_list" class="table table-striped table-bordered table-condensed width_100" cellpadding="0" cellspacing="0" border="0">
        <thead>
          <tr>
            <th class="align_left align_top"><%= t("order.list.act")%></th>
            <th class="align_left align_top"><%= t('products.list.channel_name') %></th>
            <th class="align_left align_top"><%= t('products.list.rebate_date') %></th>
            <th class="align_left align_top"><%= t('products.list.rebate_name') %></th>
            <th class="align_left align_top"><%= t('products.list.salespersion') %></th>
            <th class="align_left align_top"><%= t('products.list.create_date') %></th>
            <!--<th class="align_left align_top"><%= t('products.index.channel_status') %></th>-->
            <!--<th class="align_left align_top"><%= t('products.index.channel_change_status') %></th>-->
          </tr>
        </thead>
        <tbody>
          <%  channel.each do |channel| %>
          <tr>
            <td class="align_center sorting_1">
                        <span data-position="right" data-offset="left_0" class="xmo-tooltip l_margin_0 l_padding_5" data-id="a07843974591232836">
                          <i class="icon-setup">
                            <div class="popover xmo-popover fade in">
                              <div class="arrow"></div>
                              <div class="popover-inner xmo-popover-inner">
                                <div class="popover-content">
                                  <p><%= link_to t('products.index.channel_edit'), edit_channel_path(channel.id)%></p>
                                  <p><%= link_to t('products.index.channel_delete'), :action=>"deleted", :id=>channel.id%></p>
                                </div>
                              </div>
                            </div>
                          </i>
                        </span>
            </td>

            <td>
              <span data-position="right" data-offset="left_0" class="xmo-tooltip l_margin_0 l_padding_5">
                  <a class="blue-href" href="javascript:void(0)"><%= channel.channel_name %></a>
                    <div class="popover xmo-popover fade in">
                      <div class="arrow"></div>
                      <div class="popover-inner xmo-popover-inner">
                        <div class="popover-content">
                          <div class="tab-pane active fade in">
                            <ul class="popover-detail">
                              <li><span class="popover-detail-title"><%=t("products.list.qualification_name")%></span><span class="popover-detail-result"><%= channel.item_present?(channel.qualification_name)%></span></li>
                              <li><span class="popover-detail-title"><%=t("products.list.currency")%></span><span class="popover-detail-result"><%= channel.item_present?(channel.currency_name)%></span></li>
                              <li><span class="popover-detail-title"><%=t("products.list.contact_person")%></span><span class="popover-detail-result"><%= channel.item_present?(channel.contact_person)%></span></li>
                              <li><span class="popover-detail-title"><%=t("products.list.person_position")%></span><span class="popover-detail-result"><%= channel.item_present?(channel.position)%></span></li>
                              <li><span class="popover-detail-title"><%=t("products.list.phone_number")%></span><span class="popover-detail-result"><%= channel.item_present?(channel.phone)%></span></li>
                              <li><span class="popover-detail-title"><%=t("products.list.person_email")%></span><span class="popover-detail-result"><%= channel.item_present?(channel.email)%></span></li>
                              <li><span class="popover-detail-title"><%=t("products.list.company_adress")%></span><span class="popover-detail-result"><%= channel.item_present?(channel.company_adress)%></span></li>
                           </ul>
                            <div class="popover-ahref ">
                              <a href="<%= channel_url(:id=>channel.id)%>" class="blue-href"><%= t('clients.index.view_detail')%></a>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                </span>
            </td>

            <td>
              <span data-position="right" data-offset="left_0" class="xmo-tooltip l_margin_0 l_padding_5">
                  <a class="blue-href" href="javascript:void(0)" style="color: darkgrey;"><%= channel.rebate_date.present? ? channel.rebate_date.split(',').uniq[0] : '' %></a>
                    <div class="popover xmo-popover fade in">
                      <div class="arrow"></div>
                      <div class="popover-inner xmo-popover-inner">
                        <div class="popover-content">
                          <div class="tab-pane active fade in">
                            <ul class="popover-detail">
                              <li><span class="popover-detail-title"><%=t("products.index.other_rebate")%></span>
                                <span class="popover-detail-result">
                                <%= channel.rebate_date_totip.present? ? channel.rebate_date_totip.split(',').uniq[1..(channel.rebate_date_totip.split(',').uniq.size - 1)].join("\n") : ''%>
                              </span></li>
                            </ul>
                          </div>
                        </div>
                      </div>
                    </div>
                </span>
            </td>
            <td style="text-align:right;"><%= channel.ch_rebate.present? ? number_to_percentage(channel.ch_rebate.split(',').uniq[0], :precision => 2) : '' %></td>
            <td><%= channel.salesperson.present? ? channel.salesperson.split(',').uniq.join(',') : '-' %></td>
            <td><%= channel.created_at.present? ? channel.created_at.strftime("%Y/%m/%d %H:%M") : '' %></td>
            <!--<td><%if channel.is_cancel_examine == false %><%= t('products.index.channel_blacklist') %><%else%><%= t('products.index.channel_whitelist') %><%end%></td>-->
            <!--<td>-->
              <!--<select id="cancel_examine" class="select" data-id="<%#=channel.id%>" style="width:190px;" disname="chosen下拉">-->
                      <!--<%# if channel.is_cancel_examine == false %>-->
                      <!--<option value="1" ><%#= t('products.index.channel_whitelist') %></option>-->
                      <!--<option value="0" selected="selected" ><%#= t('products.index.channel_blacklist') %></option>-->
                      <!--<%#else%>-->
                      <!--<option value="1" selected="selected" ><%#= t('products.index.channel_whitelist') %></option>-->
                      <!--<option value="0" ><%#= t('products.index.channel_blacklist') %></option>-->
                      <!--<%#end%>-->
              <!--</select>-->
            <!--</td>-->
          </tr>
          <%end%>
        </tbody>
      </table>
    </div>


  </div>
</div>

