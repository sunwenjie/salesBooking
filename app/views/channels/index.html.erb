<script src="/js/chosen.jquery.min.js?9120" type="text/javascript"></script>
<script src="/libs/datatables-1.10.4/media/js/jquery.dataTables.min.js" type="text/javascript"></script>
<script src="/libs/datatables-1.10.4/extensions/Scroller/js/dataTables.scroller.js" type="text/javascript"></script>

<div id="index">
    <div id="ajax-tabular"></div>
    <div style="clear:both"></div>
  <div class="t_margin_0">
    
    
    
    <% if flash[:notice] %>
      <div class="notice"><%= flash[:notice] %></div>
    <% end %>

    <div id="all_channels">
      <div style="text-align:center;margin-top:20px">
        <img src="/images/ajax-loader.gif" class="loading_img">
      </div>
    </div>
</div>
</div>
    <script type="text/javascript">
        $(function(){
            $.get("<%= loading_all_channels_path%>", {}, function (data) {
                $("#all_channels").html(data)
            })
        })
    </script>







