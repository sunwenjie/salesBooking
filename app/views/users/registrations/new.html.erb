<%= javascript_include_tag "chosen.jquery.min" %>
<div class="page_title">
  <div class="row"><div class="client-row"><h1>新增用户</h1></div></div>
  </div>
<div  class ="big_section">
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name),:html=> {:class=> "form-horizontal"}) do |f|%>
      <% if devise_error_messages! != "" %>
      <div class="error sticker"><%= devise_error_messages! %></div>
     <% end %>
      <%= f.password_field :password, :style => "display:none;", :value => "iclick888" %>
      <%= f.password_field :password_confirmation, :style => "display:none;", :value => "iclick888" %>
      <%= hidden_field_tag :user_id, current_user.id %>
      <%= render :partial => "admins/form", :locals => {:f => f}%> 
  <div class="row ">
    <div class='button_area'>
      <div class="">
        <div class="outer_cancel"><button type='button' onclick="window.location.href= '/groups;">取消</button></div>
        <div class="outer_submit"><%= f.submit "新建" %></div>
      </div>
    </div>
  </div>
 <% end %>
</div>